[{"text": "Isadora Duncan --", "start": 12.719, "duration": 1.91}, {"text": "(Music) --", "start": 14.629, "duration": 1.898}, {"text": "crazy, long-legged woman\nfrom San Francisco,", "start": 16.527, "duration": 5.123}, {"text": "got tired of this country,\nand she wanted to get out.", "start": 21.65, "duration": 4.719}, {"text": "Isadora was famous\nsomewhere around 1908", "start": 27.849, "duration": 5.085}, {"text": "for putting up a blue curtain,", "start": 32.934, "duration": 4.365}, {"text": "and she would stand", "start": 37.299, "duration": 2.205}, {"text": "with her hands over her solar plexus", "start": 39.504, "duration": 3.249}, {"text": "and she would wait,", "start": 43.8, "duration": 2.181}, {"text": "and she would wait,", "start": 47.144, "duration": 2.127}, {"text": "and then, she would move.", "start": 49.271, "duration": 4.179}, {"text": "(Music)", "start": 53.45, "duration": 4.921}, {"text": "Josh and I and Somi call this piece", "start": 81.856, "duration": 5.671}, {"text": "\"The Red Circle and the Blue Curtain.\"", "start": 87.527, "duration": 4.897}, {"text": "Red circle.", "start": 96.768, "duration": 2.74}, {"text": "Blue curtain.", "start": 99.508, "duration": 3.848}, {"text": "But,", "start": 103.356, "duration": 2.273}, {"text": "this is not the beginning\nof the 20th century.", "start": 111.397, "duration": 3.975}, {"text": "This is a morning in Vancouver", "start": 115.372, "duration": 4.868}, {"text": "in 2015.", "start": 120.24, "duration": 2.374}, {"text": "(Music)", "start": 123.824, "duration": 5.779}, {"text": "(Singing)", "start": 154.404, "duration": 7.75}, {"text": "Come on, Josh!", "start": 206.068, "duration": 2.297}, {"text": "(Music)", "start": 208.365, "duration": 5.165}, {"text": "(Singing)", "start": 226.522, "duration": 8.57}, {"text": "Go!", "start": 247.082, "duration": 1.35}, {"text": "Are we there yet?", "start": 264.506, "duration": 2.634}, {"text": "I don't think so.", "start": 267.14, "duration": 2.2}, {"text": "Hey, yeah!", "start": 269.34, "duration": 2.109}, {"text": "(Music)", "start": 271.449, "duration": 4.526}, {"text": "What time is it?", "start": 289.583, "duration": 2.173}, {"text": "(Music)", "start": 291.756, "duration": 1.672}, {"text": "Where are we?", "start": 293.428, "duration": 2.683}, {"text": "Josh.", "start": 299.178, "duration": 1.962}, {"text": "Somi.", "start": 303.775, "duration": 2.476}, {"text": "Bill T.", "start": 306.251, "duration": 2.336}, {"text": "Josh.", "start": 308.587, "duration": 2.218}, {"text": "Somi.", "start": 313.315, "duration": 2.647}, {"text": "Bill T.", "start": 318.185, "duration": 3.603}, {"text": "(Applause)", "start": 362.65, "duration": 5.264}, {"text": "Yeah, yeah!", "start": 380.457, "duration": 4.0}]