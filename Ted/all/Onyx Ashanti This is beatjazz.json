[{"text": "(Music)", "start": 18.26, "duration": 10.0}, {"text": "Text: BeatJazz.", "start": 32.26, "duration": 3.0}, {"text": "BeatJazz is:", "start": 54.26, "duration": 2.0}, {"text": "1. Live looping,", "start": 56.26, "duration": 3.0}, {"text": "2. Jazz improvisation", "start": 60.26, "duration": 2.0}, {"text": "And", "start": 62.26, "duration": 3.0}, {"text": "3. \"Gestural\" sound design.", "start": 65.26, "duration": 3.0}, {"text": "Accelerometers", "start": 147.26, "duration": 2.0}, {"text": "on each hand", "start": 149.26, "duration": 2.0}, {"text": "read hand position.", "start": 151.26, "duration": 2.0}, {"text": "The color of the lights", "start": 172.26, "duration": 4.0}, {"text": "indicates which sound I am playing.", "start": 176.26, "duration": 3.0}, {"text": "Red = Drums,", "start": 182.26, "duration": 4.0}, {"text": "Blue = Bass,", "start": 186.26, "duration": 4.0}, {"text": "Green = Chords,", "start": 197.26, "duration": 5.0}, {"text": "Orange = Leads,", "start": 202.26, "duration": 2.0}, {"text": "Purple = Pads", "start": 204.26, "duration": 4.0}, {"text": "The mouthpiece", "start": 218.26, "duration": 3.0}, {"text": "consists of ...", "start": 221.26, "duration": 2.0}, {"text": "a button,", "start": 223.26, "duration": 2.0}, {"text": "two guitar picks", "start": 225.26, "duration": 4.0}, {"text": "and lots of hot glue.", "start": 229.26, "duration": 4.0}, {"text": "The heads-up display", "start": 280.26, "duration": 3.0}, {"text": "is a smartphone that displays system parameters.", "start": 283.26, "duration": 4.0}, {"text": "Why?", "start": 297.26, "duration": 3.0}, {"text": "To atomize music culture", "start": 300.26, "duration": 3.0}, {"text": "so that ALL past, present and future genres", "start": 303.26, "duration": 2.0}, {"text": "can be studied and abstracted, live.", "start": 305.26, "duration": 3.0}, {"text": "And", "start": 308.26, "duration": 2.0}, {"text": "\"BeatJazzers\"", "start": 310.26, "duration": 3.0}, {"text": "become as common as D.J.'s.", "start": 313.26, "duration": 4.0}, {"text": "But mostly ...", "start": 321.26, "duration": 5.0}, {"text": "to MAKE the future", "start": 326.26, "duration": 5.0}, {"text": "rather than wait for it.", "start": 331.26, "duration": 3.0}, {"text": "(Applause)", "start": 373.26, "duration": 10.0}]