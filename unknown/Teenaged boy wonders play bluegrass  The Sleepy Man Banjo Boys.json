[{"text": "Translator: Morton Bast\nReviewer: Thu-Huong Ha", "start": 0.0, "duration": 7.0}, {"text": "Tommy Mizzone: Tonight we're going to play you two songs.", "start": 16.317, "duration": 2.165}, {"text": "We're three brothers from New Jersey,", "start": 18.482, "duration": 2.0}, {"text": "and the funny thing is that,", "start": 20.482, "duration": 1.648}, {"text": "believe it or not, we are hooked on bluegrass", "start": 22.13, "duration": 2.64}, {"text": "and we're excited to play it for you tonight.", "start": 24.77, "duration": 2.156}, {"text": "(Music)", "start": 26.926, "duration": 115.814}, {"text": "(Applause)", "start": 142.74, "duration": 2.853}, {"text": "TM: Thank you, thank you.", "start": 145.593, "duration": 2.469}, {"text": "(Applause)", "start": 148.062, "duration": 4.285}, {"text": "Robbie Mizzone: Thank you.", "start": 152.347, "duration": 1.424}, {"text": "I'm Robbie Mizzone. I'm 13, and I play the fiddle.", "start": 153.771, "duration": 2.32}, {"text": "This is my brother, Jonny. He's 10, and he plays the banjo.", "start": 156.091, "duration": 2.56}, {"text": "And on guitar is my 14-year-old brother, Tommy.", "start": 158.651, "duration": 2.729}, {"text": "(Applause)", "start": 161.38, "duration": 3.847}, {"text": "We call ourselves the Sleepy Man Banjo Boys.", "start": 165.227, "duration": 2.955}, {"text": "(Music)", "start": 171.566, "duration": 116.493}, {"text": "(Applause)", "start": 288.059, "duration": 2.603}, {"text": "TM: Thank you.", "start": 290.662, "duration": 1.412}, {"text": "JM: Thank you all.", "start": 292.074, "duration": 0.922}, {"text": "TM: Thank you very much.", "start": 292.996, "duration": 4.0}]